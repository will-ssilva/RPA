   "source": [
    "import pdfplumber\n",
    "import pandas as pd\n",
    "\n",
    "# Abrir o PDF gerado\n",
    "with pdfplumber.open(\"dados/tabela_exemplo_vendas.pdf\") as pdf:\n",
    "    page = pdf.pages[0]\n",
    "    table = page.extract_table()\n",
    "\n",
    "# Converter para DataFrame\n",
    "df = pd.DataFrame(table[1:], columns=table[0])\n",
    "display(df)"
   ]
  }
